{
  "name": "beach-bird-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .astro,.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .astro,.js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,astro,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,astro,json,md}\"",
    "typecheck": "astro check && tsc --noEmit",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "prebuild": "npm run clean && npm run typecheck",
    "postbuild": "npm run validate:build",
    "validate:build": "astro check",
    "analyze": "astro build --analyze",
    "content:verify": "node scripts/verify-content-sync.js",
    "content:check": "node scripts/check-missing-content.js",
    "content:report": "node scripts/generate-content-report.js",
    "content:agent": "node scripts/content-sync-agent.js",
    "content:intelligent": "node scripts/intelligent-content-agent.js",
    "content:fix": "npm run content:verify && npm run content:check",
    "content:learn": "node scripts/intelligent-content-agent.js --learn"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/sitemap": "^3.5.0",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.13.2",
    "astro-seo": "^0.8.4",
    "tailwindcss": "^3.4.17",
    "sharp": "^0.33.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "glob": "^10.3.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.45.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.0.0"
  }
}
